{
  "name": "Quoter",
  "version": "0.0",
  "background": {
    "persistent": false,
    "scripts": [ "build/quoter-chrome.js" ]
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "build/quoter-chrome-content.js" ]
    }
  ],
  "permissions": [
    "contextMenus",
    "https://*/",
    "http://*/"
  ],
  "manifest_version": 2
}
